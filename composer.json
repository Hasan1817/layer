{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "brian2694/laravel-toastr": "^5.57",
        "doctrine/dbal": "^3.7.1",
        "intervention/image": "^2.7.2",
        "laravel/framework": "^10.28.0",
        "laravel/legacy-factories": "^1.3.2",
        "laravel/socialite": "^5.9.1",
        "laravel/tinker": "^2.8.2",
        "laravel/ui": "^4.2.2",
        "laravelcollective/html": "^6.4",
        "league/omnipay": "^3.2",
        "livewire/livewire": "^3.0.8",
        "mews/purifier": "^3.4.1",
        "nwidart/laravel-modules": "^10.0.0",
        "omnipay/paypal": "^3.0",
        "php-http/httplug": "^2.4",
        "socialiteproviders/apple": "^5.6.0",
        "spatie/laravel-translatable": "6.5.3",
        "spondonit/advocate-service": "^1.2.1",
        "stripe/stripe-php": "^13.0.0",
        "symfony/symfony": "^6.3.5",
        "yajra/laravel-datatables": "^10.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9.2",
        "spatie/laravel-ignition": "^2.3.1",
        "fakerphp/faker": "^1.23.0",
        "laravel/breeze": "^1.25.0",
        "laravel/sail": "^1.25.0",
        "mockery/mockery": "^1.6.6",
        "nunomaduro/collision": "^7.10.0",
        "phpunit/phpunit": "^10.4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helper.php",
            "app/Helpers/saas.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
